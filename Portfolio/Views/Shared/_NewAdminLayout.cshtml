
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Admin Panel</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/corona-free-dark/template/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/corona-free-dark/template/assets/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/corona-free-dark/template/assets/css/style.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="~/corona-free-dark/template/assets/images/favicon.png" />
</head>
<body>
    <div class="container-scroller">
        <!-- partial:../../partials/_sidebar.html -->
        <nav class="sidebar sidebar-offcanvas" id="sidebar">
            <div class="sidebar-brand-wrapper d-none d-lg-flex align-items-center justify-content-center fixed-top">
                <a class="sidebar-brand brand-logo" href="#" style="color:white; text-decoration:none">Portfolio</a>
                <a class="sidebar-brand brand-logo-mini" href="#">P</a>
            </div>
            @await Html.PartialAsync("/Views/Admin/PartialSideBar.cshtml")
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:../../partials/_navbar.html -->
            <nav class="navbar p-0 fixed-top d-flex flex-row">
                <div class="navbar-brand-wrapper d-flex d-lg-none align-items-center justify-content-center">
                    <a class="navbar-brand brand-logo-mini" href="~/corona-free-dark/template/index.html"><img src="~/corona-free-dark/template/assets/images/logo-mini.svg" alt="logo" /></a>
                </div>
                @await Html.PartialAsync("/Views/Admin/PartialNavBar.cshtml")
            </nav>
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="page-header">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#" style="text-decoration: none">@ViewBag.value2</a></li>
                                <li class="breadcrumb-item active" aria-current="page">@ViewBag.value1</li>
                            </ol>
                        </nav>
                    </div>

                    @RenderBody()
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:../../partials/_footer.html -->
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/corona-free-dark/template/assets/js/off-canvas.js"></script>
    <script src="~/corona-free-dark/template/assets/js/hoverable-collapse.js"></script>
    <script src="~/corona-free-dark/template/assets/js/misc.js"></script>
    <script src="~/corona-free-dark/template/assets/js/settings.js"></script>
    <script src="~/corona-free-dark/template/assets/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/corona-free-dark/template/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/corona-free-dark/template/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/corona-free-dark/template/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/corona-free-dark/template/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/corona-free-dark/template/assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>
    <script src="~/corona-free-dark/template/assets/js/dashboard.js"></script>
    <script src="~/corona-free-dark/template/assets/js/todolist.js"></script>



</body>
</html>
